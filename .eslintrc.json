{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": 2,
    "no-eq-null": 1,
    "no-multi-spaces": 1,
    "camelcase": 1,
    "semi": 0,
    "space-before-function-paren": 0,
    "max-len": ["error", { "code": 135, "ignoreComments": true, "ignoreUrls": true }],
    "curly": [0, "multi"],
    "import/no-duplicates": 0,
    "jest/no-test-prefixes": 0,
    "jest/no-disabled-tests": 0,
    "jest/no-done-callback": 0
  }
}
